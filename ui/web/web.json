{
	"cname":"MINA Pages 默认主题",
	"pages":[
		"/desktop/index",
		"/mobile/index"
	],
	"common":[
		"/common"
	],
	"storage": {
		"engine":"minapages",
		"options":{
			"debug":true
		},
		"pages":{
			"remote": "/web/pages",
			"url": "/static-file/{PROJECT_NAME}/user/pages",
			"origin": "/static-file/{PROJECT_NAME}/user/pages"
		},
		"binds": [
			{
				"local": "/web/assets",
				"remote": "/{PROJECT_NAME}/assets",
				"url": "/static-file/{PROJECT_NAME}/user/assets",
				"origin": "/static-file/{PROJECT_NAME}/user/assets"
			}
		]
	},

	"debug":true
}
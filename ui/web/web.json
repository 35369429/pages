{
	"cname":"XpmSNS默认主题",
	"pages":[
		"/desktop/article/index",
		"/mobile/article/detail"
	],
	"common":[
		"/moible/common"
	],
	"storage": {
		"engine":"minapages",
		"options":{
			"debug":true
		},
		"pages":{
			"remote": "/{PROJECT_NAME}/pages",
			"url": "/static-file/{PROJECT_NAME}/pages",
			"origin": "/static-file/{PROJECT_NAME}/pages"
		},
		"binds": [
			{
				"local": "/web/mobile/assets",
				"remote": "/{PROJECT_NAME}/moible/assets",
				"url": "/static-file/{PROJECT_NAME}/moible/assets",
				"origin": "/static-file/{PROJECT_NAME}/moible/assets"
			}
		]
	},
	"debug":true
}
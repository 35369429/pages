{	
	"cname":"网站首页",
	"data": {
		"categories":{
			"api":"/xpmsns/pages/category/search",
			"query":{
				"select":"category_id,name",
				"order":"priority asc",
				"children":false,
				"param":"isnav=true"
			}
		},
		"current":"{{categories.data.0}}",
		"articles":{
			"api":"/xpmsns/pages/article/search",
			"query":{
				"perpage":15,
				"order":"publish_time desc",
				"categoryId":"{{current.category_id}}",
				"page":"{{__get.page}}"
			}
		},
		"hots":[{"name":"a"},{"name":"b"},{"name":"c"}],
		"page":{
			"title":"{{current.name}}"
		}
	},
	"entries":[
		{"method":"GET", "router":"/article", "ttl":0}
	],
	
	"alias": {
		"mobile":"/mobile/article/list",
		"wechat":"/mobile/article/list",
		"wxapp": "/article"
	}
}